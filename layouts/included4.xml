<ConstraintLayout
    layout_width="90%"
    layout_height="match_parent"
>
    
    <Label
        id="block_title_4"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_constraintCenterXAlign='parent'
        layout_constraintTop_toTopOf="parent"
        layout_marginTop="16"
        textColor="black"
        value="Still more details"
        textSize="1.2em"
        textStyle="bold"
        fontFamily="Arial"
    />
    
        
    
    <TextField
        id="phone_input_4"
        layout_width="150px"
        layout_height="32px"
        layout_constraintStart_toStartOf="parent"
        layout_constraintEnd_toEndOf="parent"
        layout_constraintTop_toBottomOf="block_title_4"
        layout_margin="8"
        placeholder="Leisure Line"
        background="white"
        fontFamily="Arial"
        layout_padding="8px"
        inputType="phone"
        text=""
        value=""
        textColor="black"
        textSize="1.2em"
        textStyle="bold"
        borderRadius="10px"
    />

    
    
</ConstraintLayout>