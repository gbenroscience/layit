<ConstraintLayout
        layout_width="90%"
        layout_height="match_parent"
>
    
    <Label
        id="block_title_5"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_constraintCenterXAlign='parent'
        layout_constraintTop_toTopOf="parent"
        layout_marginTop="16"
        textColor="black"
        value="Even more details"
        textSize="1.2em"
        textStyle="bold"
        fontFamily="Arial"
    />
    
        
    
    <TextField
        id="phone_input_5"
        layout_width="150px"
        layout_height="32px"
        layout_constraintStart_toStartOf="parent"
        layout_constraintEnd_toEndOf="parent"
        layout_constraintTop_toBottomOf="block_title_5"
        layout_margin="8"
        placeholder="Church Line"
        background="white"
        fontFamily="Arial"
        layout_padding="8px"
        inputType="phone"
        text=""
        value=""
        textColor="black"
        textSize="1.2em"
        textStyle="bold"
        borderRadius="10px"
    />

    <include
            layout="included6"
            id="next_level_details"
            layout_width="250"
            layout_height="120"
            layout_marginStart="0px"
            layout_constraintCenterXAlign="parent"
            layout_constraintTop_toBottomOf="phone_input_5"
            border="0px solid gray"
            borderRadius="0em"
    />

    
    
</ConstraintLayout>